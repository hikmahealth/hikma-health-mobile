{
  "name": "hikma-health-mobile",
  "version": "0.3.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "compile": "tsc --noEmit -p . --pretty",
    "format": "prettier --write \"app/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint App.tsx app test --fix --ext .js,.ts,.tsx && npm run format",
    "patch": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:maestro": "maestro test .maestro/e2e.yaml",
    "test:coverage": "jest --coverage ; rm coverage/coverage-summary.json ; npx nyc report -t coverage --reporter=json-summary ; npx update-badge",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "patch-package",
    "bundle:visualize": "npx react-native-bundle-visualizer",
    "bundle:visualize:dev": "npx react-native-bundle-visualizer --dev",
    "build:ios:sim": "eas build --profile development --platform ios --local",
    "build:ios:dev": "eas build --profile development:device --platform ios --local",
    "build:ios:prod": "eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:dev": "eas build --profile development:device --platform android --local",
    "build:android:prod": "eas build --profile production --platform android --local",
    "build:android:release": "ENVFILE=.env npm run android:clean && npx react-native build-android --mode=release",
    "android:clean": "cd android && ./gradlew --no-daemon clean && cd ..",
    "android:release": "ENVFILE=.env npm run android:clean && npx react-native run-android --mode release",
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx server dist",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild": "npx expo prebuild",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript -w"
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.2.2",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@nozbe/watermelondb": "^0.27.1",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.0.2",
    "@rescript/core": "^1.6.1",
    "@sentry/react-native": "^5.36.0",
    "@shopify/flash-list": "^1.7.2",
    "apisauce": "^3.1.0",
    "date-fns": "^4.1.0",
    "expo": "~50.0.17",
    "expo-barcode-scanner": "~12.9.3",
    "expo-build-properties": "~0.11.1",
    "expo-camera": "~14.1.3",
    "expo-dev-client": "~3.3.12",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-localization": "~14.8.4",
    "expo-notifications": "~0.27.8",
    "expo-print": "~12.8.1",
    "expo-screen-orientation": "~6.4.1",
    "expo-sharing": "~11.10.0",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "fp-ts": "^2.16.9",
    "i18n-js": "^4.5.1",
    "i18next": "^24.2.2",
    "immer": "^10.1.1",
    "intl-pluralrules": "^2.0.1",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.474.0",
    "minisearch": "^7.1.1",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.1.0",
    "mobx-state-tree": "^7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-native": "0.73.9",
    "react-native-code-push": "^9.0.0",
    "react-native-config": "^1.5.3",
    "react-native-date-picker": "^5.0.9",
    "react-native-device-info": "^14.0.2",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-picker-select": "^9.3.1",
    "react-native-reanimated": "^3.16.7",
    "react-native-root-toast": "^3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^15.11.1",
    "react-native-web": "~0.19.6",
    "rescript": "^11.1.4",
    "rxjs": "^7.8.1",
    "use-immer": "^0.11.0",
    "usehooks-ts": "^3.1.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0",
    "@babel/plugin-transform-template-literals": "^7.0.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "better-sqlite3": "^11.8.1",
    "check-code-coverage": "^1.10.5",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-reactotron": "^0.1.1",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.4",
    "jsdom": "^26.0.0",
    "patch-package": "6.4.7",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-core-client": "2.8.11",
    "reactotron-mst": "3.1.5",
    "reactotron-react-js": "3.3.9",
    "reactotron-react-native": "5.0.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "typescript"
      ]
    }
  },
  "engines": {
    "node": ">=16"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native",
      "reactotron"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/indent": 0,
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "comma-dangle": 0,
      "multiline-ternary": 0,
      "no-undef": 0,
      "no-unused-vars": 0,
      "no-use-before-define": 0,
      "no-global-assign": 0,
      "quotes": 0,
      "react-native/no-raw-text": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": 0,
      "space-before-function-paren": 0,
      "react-native/no-inline-styles": 0,
      "prettier/prettier": [
        "error",
        {
          "no-inline-styles": false
        }
      ],
      "reactotron/no-tron-in-production": "error"
    }
  }
}